{
  "name": "@connext/vector-types",
  "version": "0.1.15-beta.21",
  "description": "TypeScript typings for common Connext types",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "iife": "dist/index-iife.js",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "proto"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && rollup -c",
    "protobuf:generate": "protoc --experimental_allow_proto3_optional --ts_opt server_grpc1,client_call,generate_dependencies --proto_path proto --ts_out ./src/grpc proto/*.proto"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "5.0.7",
    "@ethersproject/abstract-signer": "5.0.9",
    "@ethersproject/bignumber": "5.0.12",
    "@ethersproject/providers": "5.0.15",
    "@sinclair/typebox": "0.12.7",
    "evt": "1.9.12"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "2.0.0-alpha.12",
    "@protobuf-ts/protoc": "2.0.0-alpha.12",
    "ethers": "5.0.21",
    "rollup": "2.32.1",
    "rollup-plugin-typescript2": "0.28.0",
    "typescript": "4.0.5"
  }
}
