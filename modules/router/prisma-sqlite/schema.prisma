generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
  output        = "../src/generated/db-client"
}

datasource db {
  provider = "sqlite"
  url      = env("VECTOR_DATABASE_URL")
}

model QueuedUpdate {
  id             String  @id @default(uuid())
  type           String
  channelAddress String
  updateData     String
  status         String
  context        String?
}
